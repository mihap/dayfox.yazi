<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>name</key>
  <string>dayfox</string>
  <key>colorSpaceName</key>
  <string>sRGB</string>

  <key>settings</key>
  <array>
    <!-- Global colors -->
    <dict>
      <key>settings</key>
      <dict>
        <key>background</key>          <string>#eaeaea</string>
        <key>foreground</key>          <string>#1d344f</string>
        <key>caret</key>               <string>#1d344f</string>
        <key>invisibles</key>          <string>#7f848e</string>
        <key>lineHighlight</key>       <string>#dbdbdb</string>
        <key>selection</key>           <string>#dbcece</string>
        <!-- Helpful extras for readability -->
        <key>selectionForeground</key> <string>#1d344f</string>
        <key>guide</key>               <string>#ced6db</string>
        <key>activeGuide</key>         <string>#bebebe</string>
        <key>stackGuide</key>          <string>#dbdbdb</string>
      </dict>
    </dict>

    <!-- Comments -->
    <dict>
      <key>name</key>     <string>Comment</string>
      <key>scope</key>    <string>comment</string>
      <key>settings</key> <dict><key>foreground</key><string>#7f848e</string></dict>
    </dict>

    <!-- Strings -->
    <dict>
      <key>name</key>     <string>String</string>
      <key>scope</key>    <string>string</string>
      <key>settings</key> <dict><key>foreground</key><string>#618774</string></dict>
    </dict>

    <!-- Numbers -->
    <dict>
      <key>name</key>     <string>Number</string>
      <key>scope</key>    <string>constant.numeric</string>
      <key>settings</key> <dict><key>foreground</key><string>#ba793e</string></dict>
    </dict>

    <!-- Constants -->
    <dict>
      <key>name</key>     <string>Built-in constant</string>
      <key>scope</key>    <string>constant.language</string>
      <key>settings</key> <dict><key>foreground</key><string>#6ca7bd</string></dict>
    </dict>
    <dict>
      <key>name</key>     <string>User-defined constant</string>
      <key>scope</key>    <string>constant.character, constant.other</string>
      <key>settings</key> <dict><key>foreground</key><string>#6ca7bd</string></dict>
    </dict>

    <!-- Variables -->
    <dict>
      <key>name</key>     <string>Variable</string>
      <key>scope</key>    <string>variable</string>
      <key>settings</key> <dict><key>foreground</key><string>#1d344f</string></dict>
    </dict>

    <!-- Keywords / storage -->
    <dict>
      <key>name</key>     <string>Keyword</string>
      <key>scope</key>    <string>keyword</string>
      <key>settings</key> <dict><key>foreground</key><string>#b95d76</string></dict>
    </dict>
    <dict>
      <key>name</key>     <string>Storage</string>
      <key>scope</key>    <string>storage</string>
      <key>settings</key>
      <dict>
        <key>foreground</key><string>#b95d76</string>
      </dict>
    </dict>
    <dict>
      <key>name</key>     <string>Storage type</string>
      <key>scope</key>    <string>storage.
